{
    "name": "@dcc-bs/dependency-injection.bs.js",
    "version": "0.0.16",
    "description": "A lightweight, type-safe dependency injection system for JavaScript/TypeScript applications, with built-in support for Nuxt.js.",
    "license": "MIT",
    "type": "module",
    "publishConfig": {
        "registry": "https://npm.pkg.github.com/"
    },
    "exports": {
        ".": {
            "types": "./dist/types.d.mts",
            "import": "./dist/module.mjs",
            "require": "./dist/module.mjs"
        }
    },
    "main": "./dist/module.mjs",
    "types": "./dist/types.d.mts",
    "files": [
        "dist"
    ],
    "scripts": {
        "prepack": "nuxt-module-build build",
        "generate": "nuxi generate",
        "dev": "nuxi dev playground",
        "dev:build": "nuxi build playground",
        "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
        "release": "bun lint && bun test && bun prepack && changelogen --release && git push --follow-tags",
        "lint": "biome format --write",
        "check": "biome check --fix",
        "test": "vitest",
        "test:watch": "vitest --watch",
        "test:coverage": "vitest run --coverage"
    },
    "dependencies": {
        "@nuxt/kit": "^4.0.2"
    },
    "devDependencies": {
        "@biomejs/biome": "^2.1.2",
        "@nuxt/devtools": "^3.0.0",
        "@nuxt/module-builder": "^1.0.2",
        "@nuxt/schema": "^4.0.2",
        "@nuxt/test-utils": "^3.19.2",
        "@types/node": "^24.1.0",
        "@vitest/coverage-v8": "^4.0.0",
        "changelogen": "^0.6.2",
        "nuxt": "^4.0.2",
        "publish-to-git": "^1.2.0",
        "typescript": "~5.9.0",
        "vitest": "^4.0.0",
        "vue-tsc": "^3.0.4",
        "happy-dom": "^20.0.0",
        "vitest-environment-nuxt": "^1.0.1"
    }
}
