{
    "name": "@dcc-bs/dependency-injection.bs.js",
    "version": "0.0.16",
    "description": "A lightweight, type-safe dependency injection system for JavaScript/TypeScript applications, with built-in support for Nuxt.js.",
    "license": "MIT",
    "type": "module",
    "exports": {
        ".": {
            "types": "./dist/types.d.mts",
            "import": "./dist/module.mjs",
            "require": "./dist/module.mjs"
        }
    },
    "main": "./dist/module.mjs",
    "types": "./dist/types.d.mts",
    "files": [
        "dist"
    ],
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "prepack": "nuxt-module-build build",
        "generate": "nuxi generate",
        "dev": "nuxi dev playground",
        "dev:build": "nuxi build playground",
        "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
        "release": "bun lint && bun test && bun prepack && changelogen --release && git push --follow-tags",
        "lint": "biome format --write",
        "check": "biome check --fix",
        "test": "vitest",
        "test:watch": "vitest --watch",
        "test:coverage": "vitest run --coverage"
    },
    "dependencies": {
        "@nuxt/kit": "4.3.1"
    },
    "devDependencies": {
        "@biomejs/biome": "2.3.15",
        "@nuxt/devtools": "3.2.1",
        "@nuxt/module-builder": "^1.0.2",
        "@nuxt/schema": "4.3.1",
        "@nuxt/test-utils": "3.23.0",
        "@types/node": "25.2.3",
        "@vitest/coverage-v8": "4.0.18",
        "@vue/test-utils": "^2.4.6",
        "changelogen": "^0.6.2",
        "happy-dom": "20.6.1",
        "nuxt": "4.3.1",
        "publish-to-git": "^1.2.0",
        "typescript": "5.9.3",
        "vitest": "4.0.18",
        "vitest-environment-nuxt": "^1.0.1",
        "vue-tsc": "3.2.4"
    }
}
